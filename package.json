{
	"name": "ochrona",
	"displayName": "Ochrona",
	"description": "Ochrona is a Software Composition Analysis tool.",
	"version": "0.0.4",
	"icon": "resources/ochrona_logo.png",
	"publisher": "ochronasec",
	"license": "MIT",
	"author": {
		"name": "ascott"
	},
	"engines": {
		"vscode": "^1.36.0"
	},
	"galleryBanner": {
		"color": "#1C7EBF",
		"theme": "dark"
	},
	"categories": [
		"Linters",
		"Languages",
		"Other"
	],
	"keywords": [
		"ochrona",
		"security",
		"vulnerability",
		"python",
		"pip",
		"devsecops",
		"sca",
		"dependency"
	],
	"preview": false,
	"activationEvents": [
		"onCommand:extension.ochrona"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "Ochrona",
				"properties": {
					"conf.ochrona.apiKey": {
						"type": "string",
						"default": "xxxx",
						"description": "API Key for Ochrona Python Dependency Analysis tool. You can get register for an API key at https://ochrona.dev",
						"scope": "user"
					}
				}
			}
		],
		"commands": [
			{
				"command": "extension.ochrona",
				"title": "Ochrona"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.14.13",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"@types/request": "^2.48.2",
		"@types/semver": "^6.0.1",
		"request": "^2.88.0",
		"semver": "^6.3.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ochronasec/ochrona_vs.git"
	},
	"bugs": {
		"url": "https://github.com/ochronasec/ochrona_vs/issues",
		"email": "support@ochrona.dev"
	},
	"homepage": "https://github.com/ochronasec/ochrona_vs/blob/master/README.md"
}
